<tr>
  <td><%= appointment.user.name if appointment.user %></td>
  <td><%= appointment.doctor.name if appointment.doctor %></td>
  <td><%= appointment.booking_time %></td>
  <td><%= appointment.cell %></td>
  <td><%= appointment.state %></td>
  <td><%= link_to t(:edit), edit_appointment_path(appointment) %></td>
  <td><%= link_to t(:destroy), appointment, method: :delete, data: {confirm: t(:are_you_sure)} %></td>
</tr>