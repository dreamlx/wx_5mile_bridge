<tr>
  <td><%= shift.doctor.name if shift.doctor %></td>
  <td><%= shift.date %></td>
  <td><%= shift.from_time.strftime "%H:%M" %></td>
  <td><%= shift.to_time.strftime "%H:%M" %></td>
  <td><%= shift.category %></td>
  <td><%= shift.shift_type %></td>
  <td><%= link_to t(:edit), edit_shift_path(shift) %></td>
  <td><%= link_to t(:destroy), shift, method: :delete, data: {confirm: t(:are_you_sure)} %></td>
</tr>