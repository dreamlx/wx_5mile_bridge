<div class="col-md-10">
  <h3><%= t(:consult_list) %></h3>
</div>
<div class="col-md-2">
  <%= link_to t(:new_consult), new_consult_path, class: "btn btn-danger" %>
</div>
<div class="col-md-12">
  <%= render 'search' %>
</div>
<div class="col-md-12">
  <hr />
  <table class="table">
    <thead>
      <tr>
        <% Consult::HEADERS.each do |column| %>
          <th><%= Consult.human_attribute_name(column) %></th>
        <% end %>
        <th colspan="3"><%= t(:operations) %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @consults %>
    </tbody>
  </table>
</div>